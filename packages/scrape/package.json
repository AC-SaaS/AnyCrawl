{
    "name": "@anycrawl/scrape",
    "version": "1.0.0-beta.2",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "dev": "dotenv-run -r ../.. -- tsx watch ./src/Worker.ts",
        "build": "dotenv-run -r ../.. -- tsc",
        "start": "dotenv-run -r ../.. -- node dist/Worker.js",
        "clean": "rm -rf dist",
        "typecheck": "tsc --noEmit",
        "test": "NODE_OPTIONS=--experimental-vm-modules dotenv-run -r ../.. -- jest --detectOpenHandles --forceExit",
        "test:watch": "NODE_OPTIONS=--experimental-vm-modules dotenv-run -r ../.. -- jest --watch --detectOpenHandles",
        "test:coverage": "NODE_OPTIONS=--experimental-vm-modules dotenv-run -r ../.. -- jest --coverage --detectOpenHandles --forceExit"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./HttpClient": {
            "types": "./dist/HttpClient.d.ts",
            "import": "./dist/HttpClient.js",
            "default": "./dist/HttpClient.js"
        },
        "./constants": {
            "types": "./dist/constants.d.ts",
            "import": "./dist/constants.js",
            "default": "./dist/constants.js"
        },
        "./engines/*": {
            "types": "./dist/engines/*.d.ts",
            "import": "./dist/engines/*.js",
            "default": "./dist/engines/*.js"
        },
        "./managers/*": {
            "types": "./dist/managers/*.d.ts",
            "import": "./dist/managers/*.js",
            "default": "./dist/managers/*.js"
        },
        "./Utils": {
            "types": "./dist/Utils.d.ts",
            "import": "./dist/Utils.js",
            "default": "./dist/Utils.js"
        },
        "./types/*": {
            "types": "./dist/types/*.d.ts",
            "import": "./dist/types/*.js",
            "default": "./dist/types/*.js"
        }
    },
    "devDependencies": {
        "@anycrawl/typescript-config": "workspace:*",
        "@dotenv-run/cli": "^1.3.6",
        "@jest/globals": "^29.7.0",
        "@types/html-to-text": "^9.0.4",
        "@types/jest": "^29.5.12",
        "@types/node": "^22.13.10",
        "@types/supertest": "^6.0.2",
        "jest": "^29.7.0",
        "supertest": "^6.3.4",
        "ts-jest": "^29.1.2",
        "ts-jest-resolver": "^2.0.1",
        "typescript": "5.8.2"
    },
    "dependencies": {
        "@anycrawl/ai": "workspace:*",
        "@anycrawl/db": "workspace:*",
        "@anycrawl/libs": "workspace:*",
        "@anycrawl/template-client": "workspace:*",
        "@apify/utilities": "^2.11.2",
        "@crawlee/types": "^3.13.0",
        "@dotenv-run/cli": "^1.3.6",
        "bullmq": "^5.45.1",
        "cheerio": "^1.0.0",
        "crawlee": "^3.13.0",
        "html-to-text": "^9.0.5",
        "ioredis": "^5.6.0",
        "minimatch": "^10.0.3",
        "playwright": "^1.51.1",
        "puppeteer": "^24.4.0"
    }
}
