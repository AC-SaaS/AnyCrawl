{
    "name": "@anycrawl/ai",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "dotenv-run -r ../.. -- tsc --watch",
        "build": "dotenv-run -r ../.. -- tsc",
        "clean": "rm -rf dist",
        "typecheck": "tsc --noEmit",
        "test": "dotenv-run -r ../.. -- jest --detectOpenHandles --forceExit",
        "test:watch": "dotenv-run -r ../.. -- jest --watch --detectOpenHandles",
        "test:coverage": "dotenv-run -r ../.. -- jest --coverage --detectOpenHandles --forceExit",
        "test:models": "dotenv-run -r ../.. -- tsc --skipLibCheck && node temp/test.js && rm -rf temp"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "dependencies": {
        "@ai-sdk/openai": "2.0.0-beta.2",
        "@ai-sdk/openai-compatible": "1.0.0-beta.2",
        "@ai-sdk/react": "2.0.0-beta.3",
        "@ai-sdk/togetherai": "1.0.0-beta.2",
        "@anycrawl/libs": "workspace:*",
        "ai": "^5.0.0-beta",
        "install": "^0.13.0",
        "tiktoken": "^1.0.21"
    },
    "devDependencies": {
        "@anycrawl/eslint-config": "workspace:*",
        "@anycrawl/typescript-config": "workspace:*",
        "@dotenv-run/cli": "^1.3.6",
        "@types/jest": "^29.5.14",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "jest": "^29.7.0",
        "typescript": "^5.7.2"
    }
}
